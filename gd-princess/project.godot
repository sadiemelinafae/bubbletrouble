config_version=5

[application]

config/name="gd-princess"
run/main_scene="res://scenes/level_dummy.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://systems/audio/audio_layout.tres"

[autoload]

GameState="*res://systems/game_state/game_state.tscn"
AppConfig="*res://systems/menus/maaack/scenes/autoloads/app_config.tscn"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/my_extended_gltf_import_flow/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://assets-3d/": "green",
"res://assets-sound/": "green",
"res://assets/": "green",
"res://systems/": "orange"
}

[gui]

theme/custom="res://assets/ui-theme/game-ui-theme.tres"

[importer_defaults]

animation_library={
"gltf/embedded_image_handling": 0
}

